{
    "filepath": "./guides/*.md",
    "parser": "markdown",
    "printWidth": 100,
    "quoteProps": "consistent",
    "arrowParens": "always",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "crlf",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": true
}